            <h2>Pinterest</h2>
          </ion-item>

          <ion-item href="#" onclick="window.open('https://www.youtube.com/channel/UCoqygeWY9InViJE5bGSE_eg', '_system', 'location=yes'); return false;">
            <ion-avatar item-left>
              <img src="assets/img/youtube.png">
            </ion-avatar>
            <h2>YouTube</h2>
          </ion-item>

        </ion-list>

</ion-content>
