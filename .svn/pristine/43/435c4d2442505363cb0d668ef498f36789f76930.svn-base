<ion-header>

  <ion-navbar color="primary">
    <button ion-button menuToggle>
      <ion-icon name="menu"></ion-icon>
    </button>
    <ion-title>Social Media</ion-title>
  </ion-navbar>

</ion-header>

<ion-content has tabs>

<div class="">
            <img src="assets/img/social.jpg" width="100%" height="auto" style="display: block; margin-left: auto; margin-right: auto;">
        </div>


        <ion-list>
          <ion-item href="#" onclick="window.open('http://www.facebook.com/naeyc', '_system', 'location=yes'); return false;">
            <ion-avatar item-left >
              <img src="assets/img/facebook.png">
            </ion-avatar>
            <h2>Facebook</h2>
          </ion-item>

          <ion-item href="#" onclick="window.open('https://twitter.com/naeyc', '_system', 'location=yes'); return false;">
            <ion-avatar item-left>
              <img src="assets/img/twitter.png">
            </ion-avatar>
            <h2>Twitter</h2>
          </ion-item>

          <ion-item href="#" onclick="window.open('https://www.linkedin.com/company/75850/', '_system', 'location=yes'); return false;">
            <ion-avatar item-left>
              <img src="assets/img/linkedin.png">
            </ion-avatar>
            <h2>Linkedin</h2>
          </ion-item>

          <ion-item href="#" onclick="window.open('https://www.instagram.com/naeyc/', '_system', 'location=yes'); return false;">
            <ion-avatar item-left>
              <img src="assets/img/instagram.jpeg">
            </ion-avatar>
            <h2>Instagram</h2>
          </ion-item>

          <ion-item href="#" onclick="window.open('https://www.pinterest.com/naeyc/pins/', '_system', 'location=yes'); return false;">
            <ion-avatar item-left>
              <img src="assets/img/pinterest.png">
