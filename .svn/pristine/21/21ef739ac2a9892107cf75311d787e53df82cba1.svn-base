		// Saving progress
		let saving = this.loadingCtrl.create({
			spinner: 'crescent',
			content: 'Saving...'
		});

		// Alert for successful save
		let savealert = this.alertCtrl.create({
			title: 'Survey',
			subTitle: 'Survey data has been submitted.',
			buttons: ['Ok']
		});

		// Alert for failed save
