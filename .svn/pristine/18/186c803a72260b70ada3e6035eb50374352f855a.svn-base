<!--
  Generated template for the SurveyPage page.

  See http://ionicframework.com/docs/components/#navigation for more info on
  Ionic pages and navigation.
-->
<ion-header>
    <ion-navbar color="primary">
      <button ion-button menuToggle>
        <ion-icon name="menu"></ion-icon>
      </button>
      <ion-title>Survey</ion-title>
    </ion-navbar>
  </ion-header>
  
  <ion-content>


      <ion-card>
          <ion-list radio-group>
              <ion-list-header style="background:#2c3e50; color:#fff">
               1. Position on SB. 1000
              </ion-list-header>
          
          <ion-item>
              <ion-label>Supports</ion-label>
              <ion-radio checked="false" value="Yes"></ion-radio>
            </ion-item>
  
            <ion-item>
                <ion-label>Leaning Support</ion-label>
                <ion-radio checked="false" value="Yes"></ion-radio>
              </ion-item>
  
  
            <ion-item>
              <ion-label>Opposes</ion-label>
              <ion-radio checked="false" value="Yes"></ion-radio>
            </ion-item>
  
            <ion-item>
                <ion-label>Leaning Opposes</ion-label>
                <ion-radio checked="false" value="Yes"></ion-radio>
              </ion-item>
  
  
            </ion-list>
          </ion-card>
  

          <ion-card>
              <ion-list radio-group>
                  <ion-list-header style="background:#2c3e50; color:#fff">
                   2. Will they cosponsor the legislation?
                  </ion-list-header>
              
              <ion-item>
                  <ion-label>Yes</ion-label>
                  <ion-radio checked="false" value="Yes"></ion-radio>
                </ion-item>
      
                <ion-item>
                    <ion-label>No</ion-label>
                    <ion-radio checked="false" value="No"></ion-radio>
                </ion-item>
      
                </ion-list>
              </ion-card>


              <ion-card>
                  <ion-list radio-group>
                      <ion-list-header style="background:#2c3e50; color:#fff">
                       3. Will they speak publicly about the legislation?
                      </ion-list-header>
                  
                  <ion-item>
                      <ion-label>Yes</ion-label>
                      <ion-radio checked="false" value="Yes"></ion-radio>
                    </ion-item>
          
                    <ion-item>
                        <ion-label>No</ion-label>
                        <ion-radio checked="false" value="No"></ion-radio>
                      </ion-item>
          
          
                    </ion-list>
                  </ion-card>


                  <ion-card>
                      <ion-list radio-group>
                          <ion-list-header style="background:#2c3e50; color:#fff">
                           4. Have they been visited by others on the issue?
                          </ion-list-header>
                      
                      <ion-item>
                          <ion-label>Yes</ion-label>
                          <ion-radio checked="false" value="Yes"></ion-radio>
                        </ion-item>
              
                        <ion-item>
                            <ion-label>No</ion-label>
                            <ion-radio checked="false" value="No"></ion-radio>
                          </ion-item>
              
  
              
                        </ion-list>
                      </ion-card>



  <ion-list>
  <ion-card>
      <ion-card-header style="background:#2c3e50; color:#fff">
          5.	Provide the names of the staff that attended and their positions.
      </ion-card-header>
      <ion-card-content>
          <ion-textarea type="text" placeholder="Comments" rows="3"></ion-textarea>
      </ion-card-content>
    </ion-card>


    <ion-card>
        <ion-card-header style="background:#2c3e50; color:#fff">
            6.	List any observations from the meeting that you believe are important to know.
        </ion-card-header>
        <ion-card-content>
            <ion-textarea type="text" placeholder="Comments" rows="3"></ion-textarea>
        </ion-card-content>
      </ion-card>



    </ion-list>

  


    <div text-center>
      <button ion-button style="background:#78909c; color:#fff">
        SUBMIT
      </button>
    </div>
  
    <div class="spacer" style="width:320px; height: 16px;"></div>
  
 
  </ion-content>
  
