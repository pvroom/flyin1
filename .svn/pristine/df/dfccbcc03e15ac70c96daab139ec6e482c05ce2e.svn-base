page-notes {

	.iconMed  {
	  font-size: 30px;
	  padding-left:40px;
	}

	  .shadow {
		height:40px;
		margin-top: 10px;
		-webkit-box-shadow: 2px 2px 2px 2px #ccc;  /* Safari 3-4, iOS 4.0.2 - 4.2, Android 2.3+ */
		-moz-box-shadow:    2px 2px 2px 2px #ccc;  /* Firefox 3.5 - 3.6 */
		box-shadow:         2px 2px 2px 2px #ccc;  /* Opera 10.5, IE 9, Firefox 4+, Chrome 6+, iOS 5 */
	  }

	.buttonPadding {
	  background: #ef6a04;
	  height: 43px;
	  margin-top: 10px;
	  -webkit-box-shadow: 2px 2px 2px 2px #ccc;  /* Safari 3-4, iOS 4.0.2 - 4.2, Android 2.3+ */
	  -moz-box-shadow:    2px 2px 2px 2px #ccc;  /* Firefox 3.5 - 3.6 */
	  box-shadow:         2px 2px 2px 2px #ccc;  /* Opera 10.5, IE 9, Firefox 4+, Chrome 6+, iOS 5 */
	}

	.mutedBlue {
		background:rgba(50, 64, 82, 1)!important;
		color:#fff;
		font-size: 1.2em;
	}

	.mutedRed {
		background:rgba(231, 80, 60, 1)!important;
		color:#fff;
		font-size: 1.2em;
	}

}
